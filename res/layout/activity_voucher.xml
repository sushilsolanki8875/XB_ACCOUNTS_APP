    <LinearLayout android:id="@+id/ll_container_Transfer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="1dp"
        android:orientation="horizontal"
        android:visibility="visible">

        <TextView
            android:id="@+id/txt_Transfer_srno"
            android:layout_width="0dp"
            android:layout_weight="10"
            android:layout_height="wrap_content"
            android:paddingRight="6dp"
            android:paddingLeft="6dp"
            android:text="Sr No." />

        <TextView
            android:id="@+id/txt_Transfer_particular"
            android:layout_width="0dp"
            android:layout_weight="40"
            android:layout_height="wrap_content"
            android:paddingRight="6dp"
            android:paddingLeft="6dp"
            android:text="Particular" />

        <TextView
            android:id="@+id/txt_Transfer_credit"
            android:layout_width="0dp"
            android:layout_weight="15"
            android:layout_height="wrap_content"
            android:paddingRight="6dp"
            android:paddingLeft="6dp"
            android:gravity="center_horizontal"
            android:text="Credit" />

        <TextView
            android:id="@+id/txt_Transfer_debit"
            android:layout_width="0dp"
            android:layout_weight="15"
            android:layout_height="wrap_content"
            android:paddingRight="6dp"
            android:paddingLeft="6dp"
            android:gravity="center_horizontal"
            android:text="Debit" />
        
    </LinearLayout>
	
	<LinearLayout
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/Black"
            android:orientation="vertical" >
    </LinearLayout>
    
    <ListView
        android:id="@+id/lst_mainContainer"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="70" >

    </ListView>

